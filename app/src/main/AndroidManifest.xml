<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.demanda.skyline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="pt.demanda.skyline.BoardingData"
            android:label="@string/title_activity_boarding_data">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pt.demanda.skyline.DisplayBarcode"
            android:label="@string/title_activity_display_barcode">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pt.demanda.skyline.BoardingData" />
        </activity>
    </application>
</manifest>
